!/.bash_profile
!/.bashrc
!/.bash_dep
!/.vimrc
!/.vimrc.plugin
!/.gvimrc
!/.vim
!/.gitignore
!/.gitignore_global
!/.gitconfig
!/.gitconfig.local
!/.gitmodules
!/.screenrc
!/.tmux.conf
!/.zshrc
!/.zshrc.alias
!/.zshrc.custom
!/.zshrc.linux
!/.zshrc.local
!/.zshrc.osx

.vim/backups/
